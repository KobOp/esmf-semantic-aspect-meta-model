@prefix : <urn:samm:org.eclipse.samm.test:1.0.0#> .
@prefix samm: <urn:samm:org.eclipse.samm:meta-model:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:TestPropertyWithExampleValueWithoutCharacteristic a samm:Aspect ;
   samm:name "TestPropertyWithExampleValueWithoutCharacteristic" ;
   samm:properties ( :testProperty ) ;
   samm:operations ( ) .

:testProperty samm:refines :propertyToBeRefined ;
   samm:name "testProperty" ;
   samm:characteristic [
      a samm:Characteristic ;
      samm:name "TestCharacteristic" ;
      samm:dataType xsd:float
   ] .

:propertyToBeRefined a samm:Property ;
   samm:name "propertyToBeRefined" ;
   samm:exampleValue "23"^^xsd:int .
