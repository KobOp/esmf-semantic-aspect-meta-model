@prefix : <urn:bamm:com.mycompany.myapplication:1.0.2#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:MachineOnlineStatus a bamm:Aspect ;
   bamm:properties ( :machineOnlineStatus ) ;
   bamm:operations ( ) .

:machineOnlineStatus a bamm:Property ;
   bamm:description "The current machine online status."@en ;
   bamm:characteristic [
      a bamm-c:Enumeration ;
      bamm:dataType xsd:string ;
      bamm-c:values ( "ON" "OFF" "BOOTING" ) # <3>
   ] .
