@prefix : <urn:samm:org.eclipse.samm.test:TestAspect:1.0.0#> .
@prefix samm: <urn:samm:org.eclipse.samm:meta-model:1.0.0#> .
@prefix samm-c: <urn:samm:org.eclipse.samm:characteristic:1.0.0#> .

:TestProperty a samm:Aspect ;
   samm:name "TestProperty" ;
   samm:properties ( :entityList ) ;
   samm:operations ( ) .

:entityList a samm:Property ;
   samm:name "testProperty" ;
   samm:preferredName "Test Property"@en ;
   samm:preferredName "Test Property"@de ;
   samm:description "A test property."@en ;
   samm:description "Eine test Property."@de ;
   samm:characteristic [
      a samm-c:List ;
      samm:name "EntityList" ;
      samm:dataType :TestEntity
   ] .

:TestEntity a samm:Entity ;
   samm:name "TestEntity" ;
   samm:properties ( ).
