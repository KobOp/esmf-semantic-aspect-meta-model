@prefix : <urn:samm:org.eclipse.samm.test:1.0.0#> .
@prefix samm: <urn:samm:org.eclipse.samm:meta-model:1.0.0#> .
@prefix samm-c: <urn:samm:org.eclipse.samm:characteristic:1.0.0#> .

:TestAspect a samm:Aspect ;
   samm:name "TestAspect" ;
   samm:properties ( ) ;
   samm:operations ( :TestOperation ) .

:TestOperation a samm:Operation ;
   samm:name "TestOperation" ;
   samm:preferredName "Test Operation"@en ;
   samm:description "Test Operation."@en ;
   samm:input ( :input ) ;
   samm:output :output .

:input a samm:Property ;
   samm:name "input" ;
   samm:characteristic samm-c:Text .

:output a samm:Property ;
   samm:name "output" ;
   samm:characteristic samm-c:Text .
