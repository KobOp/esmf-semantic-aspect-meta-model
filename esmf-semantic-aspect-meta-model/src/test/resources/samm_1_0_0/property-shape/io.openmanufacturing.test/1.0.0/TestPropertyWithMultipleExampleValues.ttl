@prefix : <urn:samm:org.eclipse.samm.test:1.0.0#> .
@prefix samm: <urn:samm:org.eclipse.samm:meta-model:1.0.0#> .
@prefix samm-c: <urn:samm:org.eclipse.samm:characteristic:1.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:TestPropertyWithMultipleExampleValues a samm:Aspect ;
   samm:name "TestPropertyWithMultipleExampleValues" ;
   samm:properties ( :numericList ) ;
   samm:operations ( ) .

:numericList a samm:Property ;
   samm:name "numericList" ;
   samm:exampleValue "25.12"^^xsd:double ;
   samm:exampleValue "234.2"^^xsd:double ;
   samm:characteristic [
      a samm-c:List ;
      samm:name "NumericList" ;
      samm:dataType xsd:double
   ] .
